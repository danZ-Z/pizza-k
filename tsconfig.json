{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "baseUrl": "./src",
      "paths": {
         "@/*": ["./*"]
      }
    },
    "include": ["src"]
  }
  